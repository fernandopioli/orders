[run]
source = src/
omit = tests/*

[report]
exclude_lines =
    # Ignorar linhas com decorador abstractmethod
    @abstractmethod
    # Ignorar pass em classes abstratas
    pass
    # Outros padr√µes comuns a ignorar
    pragma: no cover
    def __repr__
    raise NotImplementedError
    # Ignorar TYPE_CHECKING imports (apenas para type hints)
    if TYPE_CHECKING: