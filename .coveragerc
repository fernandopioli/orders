[run]
source = src/
omit = 
    tests/*
    */migrations/*
    */venv/*
    manage.py

[report]
exclude_lines =
    @abstractmethod
    pass
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if TYPE_CHECKING:
    if __name__ == .__main__.:
show_missing = true
fail_under = 80
precision = 2

[html]
directory = htmlcov